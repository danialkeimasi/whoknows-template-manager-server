{
	"values": {
        "player": "db(footballPlayer)",
        "players": "db(footballPlayer[(footballPlayer.value > var.player.value + 10000000) | (footballPlayer.value < var.player.value - 10000000)])",
        "players_same": "db(footballPlayer[footballPlayer.value == var.player.value])"
    },
	"&choose": {
		"title": {
			"text": ["ارزش کدام بازیکن در سایت transfermarkt `var.player.value` تعیین شده است؟"]
	},
		"choice": {
			"text": ["$var.players.name"]
	},
		"answer": {
			"text": ["$var.player.name"]
	}
	},
	"&select": {
		"title": {
			"text": ["ارزش کدام بازیکنان در سایت transfermarkt `var.player.value` تعیین شده است؟"]
	},
		"choice": {
			"text": ["$var.players.choose(1).name"]
	},
		"answer": {
			"text": ["$var.players_same.name"]
	}
	},
    "datasets": ["footballPlayer"],
	"usage": ["contest"],
	"level": 2,
	"score_function": "default",
	"time_function": "default",
	"tags": ["football", "footballPlayer", "player"],
	"__state": "",
	"__test_info": {},
	"__idea": [""]

} 