{
	"values": {
        "match": "db(footballMatch)",
        "matches": "db(footballMatch[footballMatch.scores != var.match.scores], NOC)"
    },
	"$choose"          : {
		"title"        : {
			"text"       : ["نتیجه بازی `var.match.name` کدام گزینه است؟"]
	},
		"choice"        : {
			"text"       : ["$var.matches.stadium"]
	},
		"answer"        : {
			"text"       : ["$var.match.stadium"]
	}
	},
	"$write"          : {
		"title"        : {
			"text"       : ["بازی `var.match.name` با چه نتیجه ای به اتمام رسید؟"]
	},
		"answer"        : {
			"text"       : ["$var.match.stadium"]
	}
	},
	"$bool"          : {
		"title"        : {
			"text"       : ["بازی `var.match.name` با نتیجه ی `var.match.scores` به اتمام رسید.",
            "بازی `var.match.name` با نتیجه ی `var.match.choose(1).scores` به اتمام رسید."]
	}
	},
	
	"datasets": ["footballMatch"],
	"usage": ["contest"],
	"level": 2,
	"score_function": "default",
	"time_function": "default",
	"tags": ["football", "footballMatch", "match"],
	"__state": "",
	"__test_info": {},
	"__idea": [""]


} 