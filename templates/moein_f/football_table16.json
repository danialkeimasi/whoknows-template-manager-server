{
	"values": {
        "team": "db(footballTable)",
        "teams": "db(footballTable[(footballTable.league == var.team.league) & (footballTable.draws != var.team.draws)] ,NOC)"
    },
	"$choose"          : {
		"title"        : {
			"text"       : ["کدام تیم تعداد مساوی هایش در لیگ `var.team.league` برابر `var.team.tie` میباشد؟"]
	},
		"choice"        : {
			"text"       : ["var.teams.name"]
	},
		"answer"        : {
			"text"       : ["var.team.name"]
	}
	},
	
	"datasets": ["footballTable"],
	"usage": ["contest"],
	"level": 2,
	"score_function": "default",
	"time_function": "default",
	"tags": ["football", "footballTable", "league" ,"table"],
	"__state": "",
	"__test_info": {},
	"__idea": [""]

} 