{
	"values"        : {
		"player": "db(footballPlayer)",
		"players": "db(footballPlayer[(footballPlayer.year != var.player.year) & (footballPlayer.value > (50000000 - (level*4000000))) & (footballPlayer.value < (125000000 - (level*(12000000))) ] , NOC)"
	},
	"$choose"          : {
		"title"        : {
			"text"       : ["کدام بازیکن در سال `var.player.year` متولد شده است؟"]
	},
		"choice"        : {
			"text"       : ["$var.players.name"]
	},
		"answer"        : {
			"text"       : ["$var.player.name"]
	}
	},
	"$bool"          : {
		"title"        : {
			"text"       : ["`var.player.name` در سال `var.player.year` متولد شده است." , "`var.player.name` در سال `var.players.choose(1).year` متولد شده است."]
	}
	},
	
	"datasets": ["footballPlayer"],
	"usage": ["contest"],
	"level": 2,
	"score_function": "default",
	"time_function": "default",
	"tags": ["football", "footballPlayer", "player"],
	"__state": "",
	"__test_info": {},
	"__idea": [""]

} 