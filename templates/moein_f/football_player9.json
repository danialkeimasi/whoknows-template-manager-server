{
	"values": {
		"player": "db(footballPlayer)",
        "players": "db(footballPlayer[footballPlayer.height_in_cm != var.player.height_in_cm], NOC)",
        "players_same": "db(footballPlayer[footballPlayer.height_in_cm == var.player.height_in_cm], NOC)"
	},
	"&choose": {
		"title": {
			"text": ["قد کدام بازیکن برابر `var.player.height_in_cm` میباشد؟"]
	},
		"choice": {
			"text": ["$var.players.name"]
	},
		"answer": {
			"text": ["$var.player.name"]
	}
	},
	"&select": {
		"title": {
            "text": ["بازیکنانی که قدی برابر `var.player.height_in_cm` دارند را انتخاب کنید."]
	},
		"choice": {
			"text": ["$var.players.chooose(1).name"]
	},
		"answer": {
			"text": ["$var.players_same.name"]
	}
	},
	"datasets": ["footballPlayer"],
	"usage": ["contest"],
	"level": 2,
	"score_function": "default",
	"time_function": "default",
	"tags": ["football", "footballPlayer", "player"],
