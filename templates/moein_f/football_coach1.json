{
	"values": {
        "coach": "db(footballCoach)",
        "coachs": "db(footballCoach[footballCoach.nation != var.coach.nation] , NOC)"
    },
	"$choose"          : {
		"title"        : {
			"text"       : ["ملیت `var.coach.name` را انتخاب کنید؟" , "`var.coach.name` اهل چه کشوری است؟"]
	},
		"choice"        : {
			"text"       : ["$var.coachs.nation"]
	},
		"answer"        : {
			"text"       : ["$var.coach.nation"]
	}
	},
	"$write"          : {
		"title"        : {
			"text"       : ["`var.coach.name` اهل چه کشوری است؟"]
	},
		"answer"        : {
			"text"       : ["$var.coach.nation"]
	}
	},
	"$bool"          : {
		"title"        : {
			"text"       : ["`var.coach.name` اهل کشور `var.coach.nation` میباشد.",
                            "`var.coach.name` اهل کشور `var.coachs.choose(1).nation` میباشد."]
	}
	},
	
	"datasets": ["footballCoach"],
	"usage": ["contest"],
	"level": 2,
	"score_function": "default",
	"time_function": "default",
	"tags": ["football", "footballCoach", "coach"],
	"__state": "",
	"__test_info": {},
	"__idea": [""]


} 