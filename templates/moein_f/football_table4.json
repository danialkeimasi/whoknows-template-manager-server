{
	"values": {
        "team": "db(footballTable)",
        "teams": "db(footballTable[(footballTable.league == var.team.league) & (footballTable.point != var.team.point)] ,NOC)"
    },
	"$choose": {
		"title": {
			"text": ["کدام تیم در لیگ `var.team.league` امتیازش برابر `var.team.point` میباشد؟"]
	},
		"choice": {
			"text": ["var.teams.name"]
	},
		"answer": {
			"text": ["var.team.name"]
	}
	},
	
	"datasets": ["footballTable"],
	"usage": ["contest"],
	"level": 2,
	"score_function": "default",
	"time_function": "default",
	"tags": ["football", "footballTable", "league" ,"table"],
	"__state": "",
	"__test_info": {},
	"__idea": [""]

} 